import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner t=new Scanner(System.in);
		String a=t.nextLine();
		String b="";
		for(int i=0;i<a.length();i++)
		{
		    if(a.charAt(i)>='a' && a.charAt(i)<='z'|| a.charAt(i)>='A' && a.charAt(i)<='Z')
		    {
		        b=b+a.charAt(i);
		    }
		}
		System.out.println(b);
		}
}
